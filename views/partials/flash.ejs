<% if(success && success.length) {%> 
<div class="alert alert-success alert-dismissible fade show col-sm-6 offset-3 d-flex justify-content-center" role="alert">
    <%= success %> 
</div>
<% } %> 

<% if(error && error.length) {%> 
    <div class="alert alert-danger alert-dismissible fade show col-sm-6 offset-3 d-flex justify-content-center" role="alert">
        <%= error %> 
    </div>
    <% } %> 